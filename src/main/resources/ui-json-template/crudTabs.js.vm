AMIS_JSON={
    "type": "page",
    "body":{
        "type":"grid",
        "columns":[{
            "md":${width},
            "xs":${width},
            "sm":${width},
            "lg":${width},
            "name":"mainTable",
            "type": "crud",
            "api": "post:/admin/page/crudQuery/${pageCode}",
            "syncLocation": false,
            "autoFillHeight": true,
            "affixHeader":true,
            #if($openPage)
                "footerToolbar": [
                    "statistics",
                    "switch-per-page",
                    "pagination"
                ],
                "perPage":${perPage},
                "perPageAvailable":${perPageAvailable},
            #end
            "filter": {
                "title": ["<span style='line-height:30px;'>${pageName}</span>"
                    ,{
                        "type":"button",
                        "label":"刷新页面",
                        "actionType": "url",
                        "url":"",
                        "blank":false,
                        "level":"secondary",
                        "className":"m-l float-right relative jqp-top-btn"
                    },{
                        "type":"button",
                        "label":"导出Excel",
                        "actionType": "url",
                        "level":"success",
                        "url":"/admin/page/crudExport/${pageCode}${downloadParam}",
                        "className":"m-l float-right relative jqp-top-btn"
                    },{
                        "type":"button",
                        "label":"重置",
                        "actionType":"reset",
                        "level":"warning",
                        "className":"mr-1 float-right relative jqp-top-btn"
                    },
                    {
                        "type":"button",
                        "label":"查询",
                        "primary":true,
                        "actionType":"submit",
                        "className":"m-l float-right relative jqp-top-btn"
                    }
                ],
                "submitText": "",
                "mode": "horizontal",
                "body":{
                    "type":"grid",
                    "columns":${queryConfigs}
                }
            },
            "itemAction": {
                "type": "action",
                "actionType": "reload",
                "target": "${target}"
            },
            "filterTogglable": false,
            "className":"#if(${queryConfigs.replaceAll("\s","").equals("[]")}) noFilter #end",
            "headerToolbar": ${topButtons},
            "bulkActions": ${bulkButtons},
        },{
            "type":"tabs",
            "md":${tabWidth},
            "xs":${tabWidth},
            "sm":${tabWidth},
            "lg":${tabWidth},
            "tabs":${tabs}
        }]
    }
}
